 create mode 100755 tests/btrfs/272
 create mode 100644 tests/btrfs/272.out

diff --git a/tests/btrfs/272 b/tests/btrfs/272
new file mode 100755
index 000000000000..1031dbc90b04
--- /dev/null
+++ b/tests/btrfs/272
@@ -0,0 +1,122 @@
+#! /bin/bash
+# SPDX-License-Identifier: GPL-2.0
+# Copyright (c) 2022 Anand Jain
+# Copyright (c) 2022 Oracle
+#
+# FS QA Test btrfs/271
+#
+# Test btrfs write hole on RAID1
+#
+# Steps:
+#  Create a RAID1 and mount with nodatasum
+#  Create a file
+#  Fail a mirror device
+#  Overwrite the file
+#  Unfail the failed mirror device
+#  Drop the cache
+#  Read the file; from the failed mirror
+
+
+. ./common/preamble
+_begin_fstest auto quick raid
+
+. ./common/filter
+
+_supported_fs btrfs
+_require_scratch
+_require_fail_make_request
+_require_fs_sysfs devinfo/1/read_preferred
+_require_scratch_dev_pool 2
+_scratch_dev_pool_get 2
+
+get_devid()
+{
+	local logical=$1
+	local stripe=$2
+	$BTRFS_UTIL_PROG inspect-internal dump-tree -t 3 $SCRATCH_DEV | \
+		grep $logical -A 6 | \
+		$AWK_PROG "(\$1 ~ /stripe/ && \$3 ~ /devid/ && \$2 ~ /$stripe/) { print \$4 }"
+}
+
+get_device_path()
+{
+	local devid=$1
+	echo "$SCRATCH_DEV_POOL" | $AWK_PROG "{print \$$devid}"
+}
+
+enable_io_failure()
+{
+	local sysfs_bdev=`_sysfs_dev $1`
+
+	echo 100 > $DEBUGFS_MNT/fail_make_request/probability
+	echo 1000 > $DEBUGFS_MNT/fail_make_request/times
+	echo 0 > $DEBUGFS_MNT/fail_make_request/verbose
+
+	echo 1 > $sysfs_bdev/make-it-fail
+}
+
+disable_io_failure()
+{
+	local sysfs_bdev=`_sysfs_dev $1`
+
+	echo 0 > $sysfs_bdev/make-it-fail
+
+	echo 0 > $DEBUGFS_MNT/fail_make_request/probability
+	echo 0 > $DEBUGFS_MNT/fail_make_request/times
+}
+
+btrfs_read_on_devid()
+{
+	local devid=$1
+	local file=$2
+	local offset=$3
+	local size=$4
+
+	echo 3 > /proc/sys/vm/drop_caches
+	_set_fs_sysfs_attr ${SCRATCH_BDEV} devinfo/$devid/read_preferred 1
+
+	_set_fs_sysfs_attr ${SCRATCH_BDEV} read_policy device
+	$XFS_IO_PROG -c "pread -v 0 8K" $file | _filter_xfs_io_offset
+	_set_fs_sysfs_attr ${SCRATCH_BDEV} read_policy pid
+
+	_set_fs_sysfs_attr ${SCRATCH_BDEV} devinfo/$devid/read_preferred 0
+}
+
+_check_minimal_fs_size $(( 1024 * 1024 * 1024 ))
+_scratch_pool_mkfs "-d raid1 -b 1G" >> $seqres.full 2>&1
+
+_scratch_mount "-o max_inline=0,nodatacow"
+
+echo "Step 1: write to a file:"
+$XFS_IO_PROG -f -c "pwrite -W -S 0xaa 0 8K" $SCRATCH_MNT/foobar | _filter_xfs_io
+
+pagesize=$(get_page_size)
+blocksize=$(_get_block_size $SCRATCH_MNT)
+sectors_per_page=$(($pagesize / $blocksize))
+
+logical_in_btrfs=$(_btrfs_get_first_logical $SCRATCH_MNT/foobar)
+devid=$(get_devid ${logical_in_btrfs} 0)
+devpath=$(get_device_path ${devid})
+SCRATCH_BDEV=$(_real_dev $devpath)
+
+echo "Step 2: overwriting the file with one failing mirror:"
+# enable block I/O error injection
+enable_io_failure $devpath
+$XFS_IO_PROG -f -c "pwrite -W -S 0xbb 0 8K" $SCRATCH_MNT/foobar | _filter_xfs_io
+echo 3 > /proc/sys/vm/drop_caches
+# disable block I/O error injection
+disable_io_failure $devpath
+
+errs=$($BTRFS_UTIL_PROG device stats $devpath | \
+	$AWK_PROG '/write_io_errs/ { print $2 }')
+if [ $errs -ne $((4 * $sectors_per_page)) ]; then
+        _fail "Errors: $devpath $errs expected: 4"
+fi
+
+echo "Step 3: verify that the data reads back fine:"
+btrfs_read_on_devid $devid "$SCRATCH_MNT/foobar" 0 8K
+
+_scratch_dev_pool_put
+
+# success, all done
+status=0
+exit
diff --git a/tests/btrfs/272.out b/tests/btrfs/272.out
new file mode 100644
index 000000000000..cede2056ad6d
--- /dev/null
+++ b/tests/btrfs/272.out
@@ -0,0 +1,522 @@
